{
  "version": 2,
  "framework": null,
  "installCommand": "true",
  "buildCommand": "chmod +x web/build.sh && ./web/build.sh",
  "outputDirectory": "dist",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        }
      ]
    }
  ]
}
